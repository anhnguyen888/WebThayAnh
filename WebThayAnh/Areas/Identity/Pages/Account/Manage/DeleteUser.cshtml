@page "{id}"
@model WebThayAnh.Areas.Identity.Pages.Account.Manage.DeleteUserModel
@{
    ViewData["Title"] = "Delete User";
}

<h1>Delete User</h1>

<h3>Are you sure you want to delete this user?</h3>
<div>
    <h4>@Model.Email</h4>
    <form method="post">
        <input type="submit" value="Delete" class="btn btn-danger" /> |
        <a asp-page="./ManageUsers">Cancel</a>
    </form>
</div>
